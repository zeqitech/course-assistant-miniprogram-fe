<import src="/global/foot.ttml" />

<view class="page">
  <view class="width-app info">
    <s-input
      label="学期"
      value="{{ term }}"
      placeholder="请输入当前课程开课学期"
      bindinput="handleInputTerm"
    >
    </s-input>
    <button type="default" bindtap="handleSelectFile">选择文件</button>
  </view>
  <button class="width-app" type="primary" bindtap="handleAddCourse">添加课程</button>
</view>
<template is="foot"></template>