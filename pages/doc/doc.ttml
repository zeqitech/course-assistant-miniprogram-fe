<import src="/global/foot.ttml" />

<view class="page">
  <s-list
    tt:for="{{ docArray }}"
    class="item width-app"
    icon="/image/doc.png"
    title="{{ item.fileName }}"
    action="{{ item.comment ? item.score : '打分' }}"
    color="{{ item.comment?(item.score<60?'red':'green'):'blue' }}"
    data-token="{{ item.fileToken }}"
    data-title="{{ item.fileName }}"
    bindtap="navToGrade"
  ></s-list>
</view>
<template is="foot"></template>
