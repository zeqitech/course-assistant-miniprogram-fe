/* 签到按钮 */
.btn {
  width: 240rpx;
  height: 240rpx;
  border-radius: 50%;
  background-color: #2fb804;
  color: white;
  margin-top: 64px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40rpx;
}

/* 签到记录列表 */
.list {
  width: calc(100% - 48px);
  margin-top: 12px;
  display: flex;
  flex-direction: column;
}
.list .item {
  margin-top: 12px;
}
