.inputs {
  width: calc(100% - 48px);
  margin-top: 16px;
  display: flex;
  flex-direction: column;
}
.inputs .item {
  margin-top: 8px;
}
.inputs .picker {
  width: 100%;
  height: 44px;
  border: solid 1px #bbbbbb;
  border-radius: 16px;
  margin-top: 8px;
  padding-left: 12px;
  padding-right: 12px;
  background-color: #fff;
  display: flex;
  align-items: center;
  color: #888888;
}
.inputs .chat {
  margin-top: 12px;
}
.inputs .submit {
  margin-top: 24px;
}
