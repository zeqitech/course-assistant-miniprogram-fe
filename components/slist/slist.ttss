.s-list {
  width: 100%;
  height: 44px;
  border-radius: 12px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 24px;
  padding-right: 16px;
  background-color: var(--gray-scale-0);
  box-shadow: 0 4px 8px var(--box-shadow);
}
.s-list .title {
  font-size: 14px;
}
.s-list .value {
  font-size: 14px;
}
.s-list .value.green {
  color: #2fb804;
}
.s-list .value.red {
  color: #ffa497;
}
.s-list .value.yellow {
  color: #ffee97;
}
